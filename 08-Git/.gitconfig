[user]
  name = Shaun Thomas
  email = shaunthomas999@gmail.com
[http "https://github.com"]
  proxy =
[http "https://bitbucket.com"]
  proxy =
[core]
	excludesfile = /Users/shaunthomas/.gitignore
	editor = subl -n -w
	autocrlf = input
[init]
	defaultBranch = main
[gc]
# true by default. Need to set it to false for Git GUI
	autoDetach = false
[commit]
	gpgSign = false
[push]
	recurseSubmodules = check
[pull]
	rebase = true
[gui]
	pruneduringfetch = true
[smartgit "submodule"]
	fetchalways = false
	update = true
	initializenew = true
[credential]
	helper = osxkeychain
	useHttpPath = true
